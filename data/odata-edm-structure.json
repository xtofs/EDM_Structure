{
    "metadata": {
        "title": "OASIS CSDL XML Elements and Attributes Classification",
        "description": "This document lists all EDM elements and categorizes their attribute types. It is based the OASIS OData CSDL XML Representation Version 4.02 specification - Appendix B. and adds the details about values, symbolic references and constraints for attributes.",
        "version": "4.02",
        "source": "OASIS OData CSDL XML Representation Version 4.02 specification",
        "appendixUrl": "https://docs.oasis-open.org/odata/odata-csdl-xml/v4.02/csd01/odata-csdl-xml-v4.02-csd01.html#TableofXMLElementsandAttributes",
        "baseUrl": "https://docs.oasis-open.org/odata/odata-csdl-xml/v4.02/csd01/odata-csdl-xml-v4.02-csd01.html"
    },
    "attributeCategories": {
        "basic": {
            "name": "Basic Attribute",
            "description": "Simple value types (strings, booleans, numbers, identifiers)",
            "subcategories": [
                {
                    "name": "MaxLength",
                    "description": "Positive integer or symbolic value",
                    "symbolicValues": [
                        {
                            "name": "max",
                            "description": "shorthand for the maximum length supported by the service"
                        }
                    ]
                },
                {
                    "name": "Scale",
                    "description": "Non-negative integer or symbolic values",
                    "symbolicValues": [
                        {
                            "name": "floating",
                            "description": "decimal floating-point number where precision specifies significant digits (not allowed in OData 4.0 responses)"
                        },
                        {
                            "name": "variable",
                            "description": "number of digits to the right of decimal point can vary from zero to the precision value"
                        }
                    ],
                    "constraints": "Scale value MUST be less than or equal to Precision value",
                    "default": "0 if not specified",
                    "notes": "Services should use lower-case values; clients should accept values case-insensitively"
                },
                {
                    "name": "SRID",
                    "description": "Non-negative integer or symbolic value",
                    "symbolicValues": [
                        {
                            "name": "variable",
                            "description": "allows different spatial reference systems"
                        }
                    ],
                    "default": "0 for Geometry types, 4326 for Geography types if not specified"
                }
            ]
        },
        "reference": {
            "name": "Reference Attribute",
            "description": "References to other model elements represented in XML/JSON as qualified names, type names, target paths, etc. . ",
            "subcategories": [
                {
                    "name": "Absolute Reference",
                    "description": "symbolic references that reference elements anywhere in the model"
                },
                {
                    "name": "Relative Reference",
                    "description": "symbolic references that reference elements relative to a specific ancestor (or related) context element"
                }
            ]
        },
        "path": {
            "name": "Path Attribute",
            "description": "Path expressions in form of a sequence of model elements represented in XML/JSON as a string or element names with delimiters",
            "subcategories": [
                {
                    "name": "Absolute Path",
                    "description": "Paths that are naming elements by starting from the root of the model"
                },
                {
                    "name": "Relative Path",
                    "description": "Paths that are resolved relative to a specific element context"
                }
            ]
        }
    },
